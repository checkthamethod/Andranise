<% include partials/header %>
<div class="container-fluid">
    <!-- parallax1 -->
    <div class="row parallax"></div>
    <div class="container" id="contact-div">
        <div class="row text-center">
            <h1 class="primary-fontchange">Contact Me</h1>
            <h3 class="secondary-fontchange">Interested in the latest deals? Have any questions about the products listed? Send me a message! </h3>
        </div>
        <div class="row" id="contact-form">
            <form action = "" method="post">
                <div class="form-group secondary-fontchange">
                    <label for="fname">First Name</label>
                    <input class="form-control" type="text" id="fname" name="firstname" placeholder="First name">
                </div>
                <div class="form-group secondary-fontchange">
                    <label for="lname">Last Name</label>
                    <input class="form-control" type="text" id="lname" name="lastname" placeholder="Last name">
                </div>
                <div class="form-group secondary-fontchange">
                    <label for="email">Email</label>
                    <input class="form-control" type="text" id="email" name="email" placeholder="email@email.com">
                </div>
                <div class="form-group secondary-fontchange">
                    <label for="subject">Subject</label>
                    <textarea class="form-control" id="subject" name="subject" placeholder="Write here..." style="height: 200px"></textarea>
                </div>

                <input class="btn btn-primary" type="submit" value="submit">
            </form>
        </div>
    </div>
</div>
<% include partials/footer %>